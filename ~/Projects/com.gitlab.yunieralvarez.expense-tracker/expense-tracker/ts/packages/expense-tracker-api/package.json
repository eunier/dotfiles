{
	"name": "expense-tracker-api",
	"version": "0.11.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"db:migration:create": "mikro-orm migration:create --config \"./src/app/orm/options/mikro-orm.options.ts\"",
		"db:migration:down": "mikro-orm migration:down --config \"./src/app/orm/options/mikro-orm.options.ts\"",
		"db:migration:up": "mikro-orm migration:up --config \"./src/app/orm/options/mikro-orm.options.ts\"",
		"db": "mikro-orm --config \"./src/app/orm/options/mikro-orm.options.ts\"",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"start:debug": "nest start --debug --watch",
		"start:dev:nest": "nest start --watch",
		"start:dev": "bun run src/main.ts",
		"start:prod": "node dist/main",
		"snap:clear": "find . -name '*.spec.ts.snap' -type f -delete",
		"start": "nest start",
		"test:cov:file": "jest --coverage --coverageReporters=text --coverageReporters=lcov > coverage-summary.txt",
		"test:cov:watch": "jest --coverage --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:snap": "jest --updateSnapshot",
		"test:snapshot": "jest --updateSnapshot",
		"test:watch": "jest --watch",
		"test": "jest"
	},
	"dependencies": {
		"@faker-js/faker": "^9.7.0",
		"@mikro-orm/cli": "^6.4.13",
		"@mikro-orm/core": "^6.4.13",
		"@mikro-orm/migrations": "^6.4.13",
		"@mikro-orm/nestjs": "^6.1.1",
		"@mikro-orm/postgresql": "^6.4.13",
		"@nestjs/common": "^11.0.1",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/swagger": "^11.1.5",
		"ajv": "^8.17.1",
		"argon2": "^0.43.0",
		"axios": "^1.9.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"helmet": "^8.1.0",
		"install": "^0.13.0",
		"joi": "^17.13.3",
		"pg": "^8.15.6",
		"plaid": "^33.0.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"swagger-ui-express": "^5.0.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.7",
		"@types/bun": "^1.2.10",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.7",
		"@types/pg": "^8.11.14",
		"@types/supertest": "^6.0.2",
		"add": "^2.0.6",
		"dotenv": "^16.5.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^16.0.0",
		"jest": "^29.7.0",
		"jest-mock-extended": "4.0.0-beta1",
		"prettier": "^3.4.2",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0"
	},
	"jest": {
		"moduleFileExtensions": ["js", "json", "ts"],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s",
			"!**/*.migration.ts",
			"!**/*.module.ts",
			"!**/*.provider.ts",
			"!**/mikro-orm.options.ts"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"^~/(.*)$": "<rootDir>/$1"
		}
	}
}

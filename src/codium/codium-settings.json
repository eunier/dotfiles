{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "anytype",
    "autofetch",
    "Cascadia",
    "Clis",
    "comptime",
    "deinit",
    "distrobox",
    "kdbx",
    "keybinds",
    "nvim",
    "Pkgs",
    "Propo",
    "udisksctl",
    "usize"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.fontFamily": "CaskaydiaMono Nerd Font Mono, UbuntuMono Nerd Font Mono, JetBrainsMono Nerd Font Mono, 'Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 15,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.rulers": [100],
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "files.autoSave": "afterDelay",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "material-icon-theme.folders.theme": "classic",
  "outline.collapseItems": "alwaysCollapse",
  "terminal.integrated.fontFamily": "CaskaydiaMono Nerd Font Propo",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "normal",
  "todo-tree.highlights.enabled": false,
  "window.autoDetectColorScheme": true,
  "window.title": "${dirty}${rootName}${separator}${activeEditorShort}${separator}${profileName}${separator}${appName}",
  "workbench.colorTheme": "ZED One Theme Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "ZED One Theme Dark",
  "workbench.preferredLightColorTheme": "ZED One Theme Dark",
  "zig.path": "zig",
  "zig.zls.enabled": "on",
  "zig.zls.inlayHintsExcludeSingleArgument": false,
  "zig.zls.warnStyle": true
}
